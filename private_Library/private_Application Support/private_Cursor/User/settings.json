        "bash": {
            "path": "/usr/bin/flatpak-spawn",
            "args": [
                "--host",
                "--env=TERM=xterm-256color",
                "bash"
            ]
        }
    },
    "workbench.colorTheme": "Everforest Dark",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": true,
